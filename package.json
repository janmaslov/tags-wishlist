  },
  "dependencies": {
    "@elysiajs/static": "^0.8.1",
    "@elysiajs/stream": "^0.8.0",
    "@kitajs/html": "^3.0.10",
    "@kitajs/ts-html-plugin": "^1.3.3",
    "elysia": "^0.8.8",
    "kysely": "^0.27.2",
    "kysely-bun-worker": "^0.5.6"
  },
  "devDependencies": {
    "bun-types": "^1.0.21"
  },
  "module": "src/index.js"
}
